    "query": {
        "from": {
            "table": "Cms\\Blogs\\Columnists\\Code\\Tables\\Columnists",
            "alias": "cbc",
            "name": "#__cms_blogs_columnists"
        },
        "select": [
            "cbc.id",
            "cbc.user_id",
            "uu.name AS user_id_name",
            "cbc.category_id",
            "cbc_9797.title AS category_id_title",
            "cbc.published",
            "cbc.created_by",
            "uu_84.name AS created_by_name",
            "cbc.date_created",
            "cbc.modified_by",
            "uu_6516.name AS modified_by_name",
            "cbc.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "cbc.user_id = uu.id"
            },
            {
                "table": "Cms\\Blogs\\Categories\\Code\\Tables\\Categories",
                "alias": "cbc_9797",
                "name": "#__cms_blogs_categories",
                "on": "cbc.category_id = cbc_9797.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_84",
                "name": "#__users_users",
                "on": "cbc.created_by = uu_84.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_6516",
                "name": "#__users_users",
                "on": "cbc.modified_by = uu_6516.id"
            }
        ]
    }
