    "query": {
        "from": {
            "table": "Cms\\Blogs\\Tags\\Code\\Tables\\Tags",
            "alias": "cbt",
            "name": "#__cms_blogs_tags"
        },
        "select": [
            "cbt.id",
            "cbt.blog_id",
            "cb.title AS blog_id_title",
            "cbt.tag_id",
            "cbt.created_by",
            "uu.name AS created_by_name",
            "cbt.date_created",
            "cbt.modified_by",
            "uu_5429.name AS modified_by_name",
            "cbt.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Cms\\Blogs\\Code\\Tables\\Blogs",
                "alias": "cb",
                "name": "#__cms_blogs",
                "on": "cbt.blog_id = cb.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "cbt.created_by = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_5429",
                "name": "#__users_users",
                "on": "cbt.modified_by = uu_5429.id"
            }
        ]
    }
