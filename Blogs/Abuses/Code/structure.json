    "query": {
        "from": {
            "table": "Cms\\Blogs\\Abuses\\Code\\Tables\\Abuses",
            "alias": "cba",
            "name": "#__cms_blogs_abuses"
        },
        "select": [
            "cba.id",
            "cba.blog_id",
            "cb.title AS blog_id_title",
            "cba.reason",
            "cba.verified",
            "cba.abuse",
            "cba.created_by",
            "uu.name AS created_by_name",
            "cba.date_created",
            "cba.modified_by",
            "uu_1258.name AS modified_by_name",
            "cba.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Cms\\Blogs\\Code\\Tables\\Blogs",
                "alias": "cb",
                "name": "#__cms_blogs",
                "on": "cba.blog_id = cb.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "cba.created_by = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_1258",
                "name": "#__users_users",
                "on": "cba.modified_by = uu_1258.id"
            }
        ]
    }
