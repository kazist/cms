    "query": {
        "from": {
            "table": "Cms\\Contests\\Code\\Tables\\Contests",
            "alias": "cc",
            "name": "#__cms_contests"
        },
        "select": [
            "cc.id",
            "cc.title",
            "cc.description",
            "cc.start_date",
            "cc.end_date",
            "cc.no_of_days",
            "cc.image",
            "mm.title AS image_title",
            "mm.file AS image_file",
            "cc.target",
            "cc.closed",
            "cc.published",
            "cc.is_default",
            "cc.created_by",
            "uu.name AS created_by_name",
            "cc.date_created",
            "cc.modified_by",
            "uu_2166.name AS modified_by_name",
            "cc.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Media\\Media\\Code\\Tables\\Media",
                "alias": "mm",
                "name": "#__media_media",
                "on": "cc.image = mm.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "cc.created_by = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_2166",
                "name": "#__users_users",
                "on": "cc.modified_by = uu_2166.id"
            }
        ]
    }
