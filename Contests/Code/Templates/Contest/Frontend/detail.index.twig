{# empty Twig template #}
{% import "form.macro.twig" as forms %}

{{ set_assets('applications/Cms/Components/assets/js/contest.js') }}
{{ set_assets('applications/Cms/Components/assets/css/article.css') }}

{% set path_set = addTwigPath( "applications.Cms.Components.Templates", twig_object) %}
{% set item_url =  friendly_url('app=cms&com=article&subset=article&view=detail&id='~ item.id) %}

<div class="cms-content">

    <h1 class="component-header">{{ item.title }} </h1>

    <div> 
        <div class="clr"></div>
        <h3>Contest Blog</h3>
        <hr>
        <div class="contest-blogs-list">
            {% include "contest.blogs.twig" ignore missing with {'contest_id': item.id, 'offset':0 }  %}
        </div>
        <div class="clr"></div>
        <br>
    </div>

    <div class="clr"></div>
</div>



