    "query": {
        "from": {
            "table": "Cms\\Contests\\Points\\Code\\Tables\\Points",
            "alias": "ccp",
            "name": "#__cms_contests_points"
        },
        "select": [
            "ccp.id",
            "ccp.criteria_id",
            "ccc.title AS criteria_id_title",
            "ccp.blog_id",
            "cb.title AS blog_id_title",
            "ccp.value",
            "ccp.points",
            "ccp.created_by",
            "uu.name AS created_by_name",
            "ccp.date_created",
            "ccp.modified_by",
            "uu_4407.name AS modified_by_name",
            "ccp.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Cms\\Contests\\Criteria\\Code\\Tables\\Criteria",
                "alias": "ccc",
                "name": "#__cms_contests_criteria",
                "on": "ccp.criteria_id = ccc.id"
            },
            {
                "table": "Cms\\Blogs\\Code\\Tables\\Blogs",
                "alias": "cb",
                "name": "#__cms_blogs",
                "on": "ccp.blog_id = cb.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "ccp.created_by = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_4407",
                "name": "#__users_users",
                "on": "ccp.modified_by = uu_4407.id"
            }
        ]
    }
