    "query": {
        "from": {
            "table": "Cms\\Contests\\Winners\\Code\\Tables\\Winners",
            "alias": "ccw",
            "name": "#__cms_contests_winners"
        },
        "select": [
            "ccw.id",
            "ccw.blog_id",
            "cb.title AS blog_id_title",
            "ccw.category_id",
            "cbc.title AS category_id_title",
            "ccw.user_id",
            "uu.name AS user_id_name",
            "ccw.contest_id",
            "cc.title AS contest_id_title",
            "ccw.contest_season",
            "ccw.properties",
            "ccw.total_points",
            "ccw.total_values",
            "ccw.created_by",
            "ccw.date_created",
            "ccw.modified_by",
            "ccw.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Cms\\Blogs\\Code\\Tables\\Blogs",
                "alias": "cb",
                "name": "#__cms_blogs",
                "on": "ccw.blog_id = cb.id"
            },
            {
                "table": "Cms\\Blogs\\Categories\\Code\\Tables\\Categories",
                "alias": "cbc",
                "name": "#__cms_blogs_categories",
                "on": "ccw.category_id = cbc.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "ccw.user_id = uu.id"
            },
            {
                "table": "Cms\\Contests\\Code\\Tables\\Contests",
                "alias": "cc",
                "name": "#__cms_contests",
                "on": "ccw.contest_id = cc.id"
            }
        ]
    }
