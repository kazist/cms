    "query": {
        "from": {
            "table": "Cms\\Contests\\Results\\Code\\Tables\\Results",
            "alias": "ccr",
            "name": "#__cms_contests_results"
        },
        "select": [
            "ccr.id",
            "ccr.contest_id",
            "cc.title AS contest_id_title",
            "ccr.blog_id",
            "cb.title AS blog_id_title",
            "ccr.category_id",
            "cbc.title AS category_id_title",
            "ccr.user_id",
            "uu.name AS user_id_name",
            "ccr.contest_season",
            "ccr.properties",
            "ccr.total_points",
            "ccr.total_values",
            "ccr.created_by",
            "uu_1407.name AS created_by_name",
            "ccr.date_created",
            "ccr.modified_by",
            "uu_2988.name AS modified_by_name",
            "ccr.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Cms\\Contests\\Code\\Tables\\Contests",
                "alias": "cc",
                "name": "#__cms_contests",
                "on": "ccr.contest_id = cc.id"
            },
            {
                "table": "Cms\\Blogs\\Code\\Tables\\Blogs",
                "alias": "cb",
                "name": "#__cms_blogs",
                "on": "ccr.blog_id = cb.id"
            },
            {
                "table": "Cms\\Blogs\\Categories\\Code\\Tables\\Categories",
                "alias": "cbc",
                "name": "#__cms_blogs_categories",
                "on": "ccr.category_id = cbc.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "ccr.user_id = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_1407",
                "name": "#__users_users",
                "on": "ccr.created_by = uu_1407.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_2988",
                "name": "#__users_users",
                "on": "ccr.modified_by = uu_2988.id"
            }
        ]
    }
