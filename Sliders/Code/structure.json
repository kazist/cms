    "query": {
        "from": {
            "table": "Cms\\Sliders\\Code\\Tables\\Sliders",
            "alias": "cs",
            "name": "#__cms_sliders"
        },
        "select": [
            "cs.id",
            "cs.title",
            "cs.description",
            "cs.image",
            "mm.title AS image_title",
            "mm.file AS image_file",
            "cs.link",
            "cs.published",
            "cs.created_by",
            "uu.name AS created_by_name",
            "cs.date_created",
            "cs.modified_by",
            "uu_9795.name AS modified_by_name",
            "cs.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Media\\Media\\Code\\Tables\\Media",
                "alias": "mm",
                "name": "#__media_media",
                "on": "cs.image = mm.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "cs.created_by = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_9795",
                "name": "#__users_users",
                "on": "cs.modified_by = uu_9795.id"
            }
        ]
    }
