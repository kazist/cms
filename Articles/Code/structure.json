            "ca.total_points",
            "ca.published",
            "ca.moderated",
            "ca.featured",
            "ca.social_share",
            "ca.hit",
            "ca.created_by",
            "uu.name AS created_by_name",
            "ca.date_created",
            "ca.modified_by",
            "uu_1161.name AS modified_by_name",
            "ca.date_modified"
        ],
