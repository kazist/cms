    "query": {
        "from": {
            "table": "Cms\\Articles\\Code\\Tables\\Articles",
            "alias": "ca",
            "name": "#__cms_articles"
        },
        "select": [
            "ca.id",
            "ca.title",
            "ca.contest_id",
            "cc.title AS contest_id_title",
            "ca.category_id",
