    "query": {
        "from": {
            "table": "Cms\\Articles\\Contests\\Code\\Tables\\Contests",
            "alias": "cac",
            "name": "#__cms_articles_contests"
        },
        "select": [
            "cac.id",
            "cac.category_id",
            "cac_3743.title AS category_id_title",
            "cac.contest_id",
            "cc.title AS contest_id_title",
            "cac.created_by",
            "uu.name AS created_by_name",
            "cac.date_created",
            "cac.modified_by",
            "uu_8866.name AS modified_by_name",
            "cac.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Cms\\Articles\\Categories\\Code\\Tables\\Categories",
                "alias": "cac_3743",
                "name": "#__cms_articles_categories",
                "on": "cac.category_id = cac_3743.id"
            },
            {
                "table": "Cms\\Contests\\Code\\Tables\\Contests",
                "alias": "cc",
                "name": "#__cms_contests",
                "on": "cac.contest_id = cc.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "cac.created_by = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_8866",
                "name": "#__users_users",
                "on": "cac.modified_by = uu_8866.id"
            }
        ]
    }
